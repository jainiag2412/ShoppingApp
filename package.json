{
  "name": "shoppingapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "ts:check": "tsc",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start-clear-cache": "react-native start --reset-cache",
    "ios-bundle": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "start-android": "react-native run-android",
    "start-ios": "react-native run-ios",
    "android-build": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "pod-install": "cd ./ios && pod install",
    "pod-update": "cd ./ios && pod update"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react": "~18.2.14",
    "axios": "^1.5.0",
    "eslint-config-prettier": "^9.0.0",
    "expo": "~49.0.8",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-simple-toast": "^3.0.1",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "private": true
}
